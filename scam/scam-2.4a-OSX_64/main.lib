(define (copy items)
    (cond
        ((null? items) nil)
        (else (cons (car items) (copy (cdr items))))
        )
    )

(define (deep-copy items)
    (cond
        ((pair? items) (cons (deep-copy (car items)) (deep-copy (cdr items))))
        (else items)
        )
    )

