(define (member x items)
    (cond
        ((null? items)
            nil
            )
        ((and (object? x) (eq? x (car items)))
            items
            )
        ((object? x)
            (member x (cdr items))
            )
        ((equal? x (car items))
            items
            )
        (else
            (member x (cdr items))
            )
        )
    )

