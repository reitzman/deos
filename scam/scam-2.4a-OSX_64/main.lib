(define (stream-car s) (car s))

(define (stream-cdr s)
    (define scdr (cdr s))
    (define scddr (cdr scdr))
    (cond
        ((null? (car scddr)) (car scdr))
        (else
            (set-car! scdr (apply eval scdr))
            (set-car! scddr nil)
            (car scdr)
            )
        )
    )
